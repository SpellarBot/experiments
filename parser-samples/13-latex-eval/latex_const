/* Greek letters */    
"\\alpha"      return 'ALPHA'
"\\beta"       return 'BETA'
"\\chi"        return 'CHI'
"\\delta"      return 'DELTA'
"\\epsilon"    return 'EPSILON'
"\\varepsilon" return 'VAREPSILON'
"\\eta"        return 'ETA'
"\\gamma"      return 'GAMMA'
"\\iota"       return 'IOTA'
"\\kappa"      return 'KAPPA'
"\\lambda"     return 'LAMBDA'
"\\mu"         return 'MU'
"\\nu"         return 'NU'
"\\omega"      return 'OMEGA'
"\\phi"        return 'PHI'
"\\varphi"     return 'VARPHI'
"\\pi"         return 'PI'
"\\psi"        return 'PSI'
"\\rho"        return 'RHO'
"\\sigma"      return 'SIGMA'
"\\tau"        return 'TAU'
"\\theta"      return 'THETA'
"\\upsilon"    return 'UPSILON'
"\\xi"         return 'XI'
"\\zeta"       return 'ZETA'
"\\Delta"      return 'DELTA_CAP'
"\\Gamma"      return 'GAMMA_CAP'
"\\Lambda"     return 'LAMBDA_CAP'
"\\Omega"      return 'OMEGA_CAP'
"\\Phi"        return 'PHI_CAP'
"\\Pi"         return 'PI_CAP'
"\\Psi"        return 'PSI_CAP'
"\\Sigma"      return 'SIGMA_CAP'
"\\Theta"      return 'THETA_CAP'
"\\Upsilon"    return 'UPSILON_CAP'
"\\Xi"         return 'XI_CAP'
/* Hebrew Letters */'
"\\aleph"      return 'ALEPH'
"\\beth"       return 'BETH'
"\\daleth"     return 'DALETH'
"\\gimel"      return 'GIMEL'